{
    "name": "slm/queue-sqs",
    "description": "Zend Framework 2 module that integrates with Amazon SQS queuing system",
    "license": "BSD-3-Clause",
    "type": "library",
    "keywords": [
        "zf2",
        "queue",
        "job",
        "amazon aws",
        "sqs"
    ],
    "homepage": "https://github.com/juriansluiman/SlmQueueSqs",
    "authors": [
        {
            "name": "Jurian Sluiman",
            "email": "jurian@juriansluiman.nl",
            "homepage": "http://juriansluiman.nl"
        },
        {
            "name": "MichaÃ«l Gallego",
            "email": "mic.gallego@gmail.com",
            "homepage": "http://www.michaelgallego.fr"
        }
    ],
    "require": {
        "php": "^5.5 || ^7.0",
        "zendframework/zend-eventmanager": "^2.6 || ^3.0",
        "zendframework/zend-servicemanager": "^2.7 || ^3.0",
        "zendframework/zend-stdlib": "^2.2 || ^3.0",
        "slm/queue": "^1.0",
        "aws/aws-sdk-php-zf2": "^3.0"
    },
    "require-dev": {
        "zendframework/zend-config": "^2.2",
        "zendframework/zend-modulemanager": "^2.5",
        "zendframework/zend-view": "^2.5",
        "zendframework/zend-log": "^2.5",
        "zendframework/zend-i18n": "^2.5",
        "zendframework/zend-serializer": "^2.5",
        "zendframework/zend-mvc": "^2.5",
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "^2.1"
    },
    "extra": {
        "zf": {
            "module": "SlmQueueSqs\\Module"
        }
    },
    "autoload": {
        "psr-4": {
            "SlmQueueSqs\\": "src/"
        },
        "classmap": [
            "./Module.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "SlmQueueSqsTest\\": "tests/"
        }
    }
